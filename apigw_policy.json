{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Principal": "*",
        "Action": "execute-api:Invoke",
        "Resource": "arn:aws:execute-api:REGION:ACCOUNT_ID:API_ID/*/*/*",
        "Condition": {
          "IpAddress": {
            "aws:SourceIp": "GITHUB_IP_RANGES"
          }
        }
      }
    ]
  }